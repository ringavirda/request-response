{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: Edge",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Debug: Server",
      "program": "${workspaceFolder}/dist/server.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal"
    }
  ]
}
